.contents {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 180rpx;
}

.app-icon {
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 50rpx;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
